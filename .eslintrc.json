{
  "env": {
    "browser": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "parserOptions": {
    "ecmaVersion": "2017",
    "sourceType": "module"
  },
  "overrides": [
    {
      "files": [ "*.spec.js" ],
      "env": { "jest": true }
    }
  ],
  "plugins": [
    "import"
  ],
  "rules": {
    "no-console": 0
  }
}
